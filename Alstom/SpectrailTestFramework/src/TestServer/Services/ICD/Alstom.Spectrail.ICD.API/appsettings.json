{
  "SpectrailMongoDatabaseSettings": {
    "ConnectionString": "mongodb://localhost:27017",
    "ICDDatabase": "ICDDB",
    "ICDEntityRegistry": "ICDEntityRegistry",
    "ICDEntityMapping": "ICDEntityMapping"
  },
  "RedisConfig": {
    "ConnectionString": "localhost:6379",
    "Key": "LastFolderHash"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Settings": {
    "ICD_FOLDER_PATH": "DATA/ICDs/",
    "DynamicEntityFilters": {
      "trdp_icd_generated.xlsx": [
        "MPU",
        "BCH"
      ],
      "trdp_icd_cc.xlsx": [
        "BCE",
        "VAC"
      ],
      "default": []
    }
  },
  "FeatureFlags": {
    "EnableChecksumValidation": true,
    "EnableEagerLoading": false,
    "EnableMiddlewarePreloading": true
  },
  "AllowedHosts": "*"
}
